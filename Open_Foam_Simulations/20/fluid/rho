FoamFile
{
    format      binary;
    class       volScalarField;
    location    "20/fluid";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    airInlet
    {
        type            calculated;
        value           uniform 1000;
    }
    airOutlet
    {
        type            calculated;
        value           uniform 1000;
    }
    airWalls
    {
        type            calculated;
        value           uniform 1000;
    }
    fluid_to_solid
    {
        type            calculated;
        value           uniform 1000;
    }
}


