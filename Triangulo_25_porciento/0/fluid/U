FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (1e-3 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"


    ".*"
    {
        type            noSlip;
    }
    
}

